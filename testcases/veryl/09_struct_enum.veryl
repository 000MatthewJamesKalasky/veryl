module Module09 {
    // struct declaration
    struct A {
        a  : logic<10>,
        aa : logic<10>,
        aaa: u32      ,
    }

    // enum declaration
    enum B: logic<2> {
        X = 1,
        Y = 2,
        Z,
    }

    var a: A;
    var b: B;

    assign a.a   = 1;
    assign a.aa  = 1;
    assign a.aaa = 1;
    assign b     = B::X;
}
