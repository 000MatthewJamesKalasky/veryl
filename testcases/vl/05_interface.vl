// interface declaration
interface InterfaceA #(
    // interface parameter
    parameter  a  : u32  = 1,
    parameter  aa : u32  = 1,
    localparam aaa: u32  = 1,
) {
    // parameter declaration
    parameter  a : u32  = 1;
    localparam aa: u64  = 1;

    // variable declaration
    a  : logic         ;
    aa : logic [10]    ;
    aaa: bit   [10][10];

    // modport declaration
    modport a {
        a  : input ,
        aa : output,
        aaa: inout ,
    }
}
