// interface declaration
interface InterfaceA #(
    // interface parameter
    parameter  a  : u32  = 1,
    parameter  aa : u32  = 1,
    localparam aaa: u32  = 1,
) {
    // parameter declaration
    parameter  b : u32  = 1;
    localparam bb: u64  = 1;

    // let declaration
    let a  : logic         ;
    let aa : logic [10]    ;
    let aaa: bit   [10][10];

    // modport declaration
    modport a {
        a  : input ,
        aa : output,
        aaa: inout ,
    }
}
