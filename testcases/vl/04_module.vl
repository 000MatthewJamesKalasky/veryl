// module declaration
module ModuleA #(
    // module parameter
    parameter  a : u32  = 1,
    localparam aa: u32  = 1,
) (
    // module port
    a  : input  logic [10],
    aa : output logic [10],
    aaa: inout  logic [10],
) {
    // parameter declaration
    parameter  b : u32  = 1;
    localparam bb: u64  = 1;

    // variable declaration
    let b  : logic         ;
    let bb : logic [10]    ;
    let bbb: bit   [10][10];
}
